<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label fx:id="loginLabel" layoutX="166.0" layoutY="46.0" text="Login">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="37.0" layoutY="117.0" text="Username">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="37.0" layoutY="169.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="timezoneLabel" layoutX="37.0" layoutY="275.0" text="Time Zone">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="usernameLogin" layoutX="151.0" layoutY="114.0" />
      <TextField fx:id="passwordLogin" layoutX="151.0" layoutY="166.0" />
      <Button fx:id="loginButton" layoutX="155.0" layoutY="215.0" mnemonicParsing="false" onAction="#loginButtonClicked" text="Login" />
      <Button fx:id="exitButton" layoutX="254.0" layoutY="215.0" mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="25.0" prefWidth="46.0" text="Exit" />
      <TextField fx:id="loginTimeZone" editable="false" layoutX="151.0" layoutY="272.0" />
   </children>
</AnchorPane>
